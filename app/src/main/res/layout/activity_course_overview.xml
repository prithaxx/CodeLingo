<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/relativeLayout"
	android:layout_width="match_parent"
	android:layout_height="wrap_content">

	<View
		android:id="@+id/view3"
		android:layout_width="0dp"
		android:layout_height="wrap_content"
		android:background="@drawable/login_background"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<View
		android:id="@+id/view5"
		android:layout_width="0dp"
		android:layout_height="90dp"
		android:background="@drawable/navbar_shape"
		android:translationZ="2dp"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<ImageView
		android:id="@+id/imageView3"
		android:layout_width="74dp"
		android:layout_height="81dp"
		android:contentDescription="codelingo_logo_1"
		android:src="@drawable/codelingo_logo"
		android:translationZ="3dp"
		app:layout_constraintBottom_toBottomOf="@+id/view5"
		app:layout_constraintEnd_toEndOf="@+id/view5"
		app:layout_constraintHorizontal_bias="0.38"
		app:layout_constraintStart_toStartOf="@+id/view5"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/textView"
		android:layout_width="205dp"
		android:layout_height="59dp"
		android:text="@string/codelingo_string"
		android:textColor="@color/_bg__surface_pro_8___1_ek2_color"
		android:textSize="40sp"
		android:translationZ="3dp"
		app:layout_constraintBottom_toBottomOf="@+id/view5"
		app:layout_constraintEnd_toEndOf="@+id/view5"
		app:layout_constraintStart_toStartOf="@+id/view5"
		app:layout_constraintTop_toTopOf="@+id/view3" />

	<ImageView
		android:id="@+id/imageView2"
		android:layout_width="19.44dp"
		android:layout_height="18.23dp"
		android:contentDescription="vector"
		android:src="@drawable/hamburger_menu"
		android:visibility="gone"
		tools:visibility="gone" />

	<androidx.fragment.app.FragmentContainerView
		android:id="@+id/fragmentContainerView3"
		android:name="CodeLinguists.codelingo.ui.cont_CourseOverview"
		android:layout_width="0dp"
		android:layout_height="0dp"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toBottomOf="@+id/view5" />

</androidx.constraintlayout.widget.ConstraintLayout>