					android:layout_marginTop="0dp"
					android:layout_width="1280dp"
					android:layout_height="800dp" />

				<View   android:id="@+id/window"
					android:background="@drawable/login_background"
					android:layout_width="1280dp"
					android:layout_height="800dp" />

				<View   android:id="@+id/navbar"
					android:background="@drawable/navbar_shape"
					 android:layout_marginStart="0dp"
					android:layout_marginTop="0dp"
					android:layout_width="1280dp"
					android:layout_height="90dp" />

				<ImageView android:id="@+id/codelingo_logo_1"
					android:src="@drawable/codelingo_logo"
					android:contentDescription="codelingo_logo_1"
